{
  "name": "Image",
  "base": "CommonModel",
  "idInjection": true,
  "strict": true,
  "properties": {
    "status": {
      "type": "string",
      "required": "true",
      "default": "active"
    },
    "type": {
      "type": "string",
      "required": true
    }
  },
  "validations": [],
  "relations": {
    "user": {
      "type": "belongsTo",
      "model": "user"
    },
    "object": {
      "type": "belongsTo",
      "polymorphic": {
        "foreignKey": "objectId",
        "discriminator": "type"
      }
    }
  },
  "acls": [
    {
      "principalType": "ROLE",
      "principalId": "$everyone",
      "permission": "DENY"
    },
    {
      "principalType": "ROLE",
      "principalId": "$authenticated",
      "permission": "ALLOW",
      "property": "upload"
    }
  ],
  "methods": [],
  "indexes": {
    "status_type_created_index": {
      "status": 1,
      "type": 1,
      "created": -1
    },
    "status_created_index": {
      "status": 1,
      "created": -1
    },
    "status_userId_created_index": {
      "status": 1,
      "userId": 1,
      "created": -1
    }
  }
}
