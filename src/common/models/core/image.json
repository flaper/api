{
  "name": "Image",
  "base": "CommonModel",
  "idInjection": true,
  "strict": true,
  "properties": {
    "type": {
      "type": "string",
      "required": true
    }
  },
  "validations": [],
  "relations": {
    "user": {
      "type": "belongsTo",
      "model": "user"
    },
    "object": {
      "type": "belongsTo",
      "polymorphic": {
        "foreignKey": "objectId",
        "discriminator": "type"
      }
    }
  },
  "acls": [
    {
      "principalType": "ROLE",
      "principalId": "$everyone",
      "permission": "DENY"
    },
    {
      "principalType": "ROLE",
      "principalId": "$authenticated",
      "permission": "ALLOW",
      "property": "upload"
    }
  ],
  "methods": []
}
