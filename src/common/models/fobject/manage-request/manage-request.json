{
  "name": "ManageRequest",
  "base": "CommonModel",
  "idInjection": true,
  "options": {
    "validateUpsert": true
  },
  "properties": {
    "status": {
      "type": "string",
      "default": "active",
      "required": true
    },
    "name": {
      "type": "string",
      "required": true
    },
    "position": {
      "type": "string"
    },
    "phone": {
      "type": "string"
    },
    "email": {
      "type": "string"
    }
  },
  "validations": [],
  "relations": {
    "user": {
      "type": "belongsTo",
      "model": "user"
    },
    "subject": {
      "type": "belongsTo",
      "polymorphic": true
    }
  },
  "acls": [
    {
      "principalType": "ROLE",
      "principalId": "$everyone",
      "permission": "DENY"
    },
    {
      "accessType": "READ",
      "principalType": "ROLE",
      "principalId": "$owner",
      "permission": "ALLOW"
    },
    {
      "principalType": "ROLE",
      "principalId": "$authenticated",
      "permission": "ALLOW",
      "action": "create"
    },
    {
      "principalType": "ROLE",
      "principalId": "super",
      "permission": "ALLOW"
    }
  ],
  "methods": {},
  "indexes": {
    "created_status_index": {
      "created": -1,
      "status": 1
    },
    "user_index": {
      "userId": -1
    }
  }
}
